type Depiction {
  res: String
  error: String
}

type User {
  username: String!
  identifier: String!
}

input Registration {
  username: String!
  password: String!
}

input Login {
  username: String!
  password: String!
}

type Query {
  ping: String!
}

type Mutation {
  register(input: Registration!): Depiction!
  login(input: Login!): String!
}
